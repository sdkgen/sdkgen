{
    "name": "sdkgen",
    "displayName": "sdkgen",
    "description": "sdkgen language support",
    "version": "0.0.10",
    "publisher": "cubos",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages", "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "sdkgen",
            "aliases": ["sdkgen", "sdkgen"],
            "extensions": [".sdkgen"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sdkgen",
            "scopeName": "source.sdkgen",
            "path": "./syntaxes/sdkgen.tmLanguage.json"
        }],
        "snippets": [{
            "language": "sdkgen",
            "path": "./snippets/sdkgen.json"
        }]
    }
}
