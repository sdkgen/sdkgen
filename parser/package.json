{
  "name": "@sdkgen/parser",
  "version": "0.0.0",
  "description": "",
  "main": "./src/index.js",
  "exports": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "TZ=UTC NODE_OPTIONS='--no-warnings --experimental-vm-modules' jest --passWithNoTests",
    "eslint:fix": "eslint --fix '**/*.ts'",
    "eslint:check": "eslint '**/*.ts'",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdkgen/sdkgen.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sdkgen/sdkgen/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/sdkgen/sdkgen#readme",
  "devDependencies": {
    "@cubos/eslint-config": "^2.0.0",
    "@types/jest": "^29.0.0",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.0.0",
    "typescript": "^4.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": [
        "esbuild-jest",
        {
          "sourcemap": "inline",
          "target": "es2021",
          "format": "esm"
        }
      ]
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testRegex": "spec/.*\\.spec\\.ts$",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/spec/"
    ]
  }
}
